@page
@model IS_Proj_HIT.Areas.Identity.Pages.Account.IncorrectAnswersModel
@{
    ViewData["Title"] = "Incorrect Answers";
}

<h1>@ViewData["Title"]</h1>
<h4>At least one of your answers was incorrect</h4>
<div class="row">
    <div class="col-md-5">
        <form method="post">
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Try Again</button>
            </div>
        </form>
    </div>
 </div>
