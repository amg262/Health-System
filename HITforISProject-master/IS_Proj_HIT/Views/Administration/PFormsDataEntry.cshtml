@model IEnumerable<string>
@using IS_Proj_HIT.Services

@{
    ViewBag.Title = "Misc Data Entry";
}

<div class="row my-3">
<h3>
    <a class="btn btn-primary" asp-action="Index">
        <i class="fa fa-arrow-left "></i>
    </a>
     Misc Data Entry Management
</h3>
</div>


<div class="container">
    <div class="row">
        @foreach (var entityName in Model)
        {
            <div class="col-sm-4 col-lg-3 mb-3">
                <div class="card">
                    <div class="card-body text-center">
                        <a class="btn btn-outline-primary" asp-page="/PForms/@entityName/Index">@ConversionService.ConvertTableName(@entityName)</a>
                    </div>
                </div>
            </div>
        }
        <div class="col-sm-4 col-lg-3 mb-3">
                <div class="card">
                    <div class="card-body text-center">
                        Departments are handled under the Encounter Section
                    </div>
                </div>
            </div>
    </div>
</div>