@page
@model IS_Proj_HIT.EditModelAdmitType

@{
    ViewData["Title"] = "Edit AdmitType";
}

<h1>Edit AdmitType</h1>

<h4>AdmitType</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AdmitType.AdmitTypeId" />
            <div class="form-group">
                <label asp-for="AdmitType.WiPopCode" class="control-label"></label>
                <input asp-for="AdmitType.WiPopCode" class="form-control" maxlength="1" />
                <span asp-validation-for="AdmitType.WiPopCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdmitType.Description" class="control-label"></label>
                <input asp-for="AdmitType.Description" class="form-control" maxlength="25" />
                <span asp-validation-for="AdmitType.Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdmitType.Explaination" class="control-label"></label>
                <input asp-for="AdmitType.Explaination" class="form-control" />
                <span asp-validation-for="AdmitType.Explaination" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdmitType.LastModified" class="control-label"></label>
                <input asp-for="AdmitType.LastModified" class="form-control" readonly />
                <span asp-validation-for="AdmitType.LastModified" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
